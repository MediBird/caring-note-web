{
  "name": "caring-note-web",
  "private": true,
  "version": "0.9.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "type-check": "tsc -p tsconfig.app.json --noEmit",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json}\" --ignore-path .prettierignore",
    "generate:api": "openapi-generator-cli generate -i https://caringnote.co.kr/api/v1/api-docs -g typescript-axios -o src/api --additional-properties=apiPackage=api,modelPackage=models,withSeparateModelsAndApi=true,apis,supportingFiles",
